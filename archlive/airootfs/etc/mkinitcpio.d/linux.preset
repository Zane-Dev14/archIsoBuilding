# mkinitcpio preset file for the custom 'linux-mits-capstone' package on ArchISO

PRESETS=('default' 'fallback')

ALL_config="/etc/mkinitcpio.conf"
ALL_kver="6.16.4-2-mits-capstone-Mits-Capstone"

# Default initramfs
default_image="/boot/initramfs-linux-mits-capstone.img"
default_options=""

# Fallback initramfs
fallback_image="/boot/initramfs-linux-mits-capstone-fallback.img"
fallback_options="-S autodetect"
